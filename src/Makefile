all: tokenizer

# asciidraw (target) depends on all object files (prerequisites)                
tokenizer: $(OBJS)
        cc -o tokenizer $(CFLAGS) $(OBJS)

# all object files built from c files that include draw.h                       
$(OBJS): history.h

# deletes files generated by compilation                                        
clean:
        rm -f *.o tokenizer

run:tokenizer
        ./tokenizer

